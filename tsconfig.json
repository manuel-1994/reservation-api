{
  "compilerOptions": {
    "lib": ["ESNext"],
    "target": "ESNext",
    "types": ["bun-types"],
    "module": "Preserve",
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  }
}
